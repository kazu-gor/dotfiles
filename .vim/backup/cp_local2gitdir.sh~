# remove
rm -rf .config/nvim/
rm -rf .vim/backup/

# copy
## directory
cp -R ~/.config/nvim/ .config/nvim
cp -R ~/.vim/backup/ .vim/backup/

## file
cp ~/.zshrc .zshrc
cp ~/.tmux.conf .tmux.conf
cp ~/.zsh_history .zsh_history
cp ~/Library/Application\ Support/Code/User/keybindings.json .config/Code/User/keybindings.json
cp ~/Library/Application\ Support/Code/User/settings.json .config/Code/User/settings.json

# tree file
tree ~/.vim/autoload/ > .vim/autoload_tree.md
tree ~/.vim/python3/ > .vim/python3_tree.md
